{
 "name": "sap-capire-mail-app",
 "version": "1.0.0",
 "description": "A simple CAP project.",
 "repository": "<Add your repository here>",
 "license": "UNLICENSED",
 "private": true,
 "dependencies": {
  "@cap-js/hana": "^1",
  "@sap/cds": "^8",
  "@sap/xssec": "^4",
  "@sendgrid/mail": "^8.1.4",
  "dotenv": "^16.4.5",
  "express": "^4"
 },
 "devDependencies": {
  "@cap-js/cds-types": "^0.6",
  "@cap-js/sqlite": "^1.7.6",
  "@sap/cds-dk": "^8"
 },
 "scripts": {
  "start": "cds-serve"
 },
 "cds": {
  "requires": {
   "MailService": {
    "kind": "odata",
    "model": "srv/*"
   },
   "ProductService": {
    "kind": "odata",
    "model": "srv/*"
   },
   "[production]": {
    "db": "hana",
    "impl": "@cap-js/hana",
    "deploy-format": "hdbtable",
    "auth": "xsuaa"
   },
   "[hybrid]": {
    "db": "hana"
   },
   "[development]": {
    "db": "sqlite"
   }
  },
  "sql": {
   "native_hana_associations": false
  }
 }
}
